@model IEnumerable<ViewModels.Client>

<h2>Items</h2>
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Price</th>
		</tr>
	</thead>

	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.ID</td>
				<td>@item.Name</td>
				<td>@item.Type</td>
			</tr>
		}
	</tbody>
</table>